{
    "name": "courage/orangemoneyussd",
    "description": "A Laravel package for USSD ORANGE MONEY payment API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Courage\\OrangeMoney\\": "src/",
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "courage mahwou",
            "email": "ndimbucourage@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Courage\\OrangeMoney\\Provider\\UssdOrangeMoneyServiceProvider"
            ],
          "aliases" : {
            "UssdOrangeMoney": "Courage\\OrangeMoney\\Facade\\UssdOrangeMoney"
          }
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
